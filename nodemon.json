{
  "signal": "SIGHUP",
  "env": {
    "NODE_ENV": "development"
  },
  "watch": [
    "src/pages/*",
    "src/styles/*",
    "src/templates/*",
    "src/manifest.json",
    "webpack",
    ".babelrc",
    ".eslintrc",
    ".eslintignore"
  ],
  "exec": "npx webpack --config ./webpack/webpack.config.dev.js"
}
